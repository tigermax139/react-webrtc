{
  "name": "react-webrtc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "server": "node ./server.js"
  },
  "dependencies": {
    "antd": "^3.24.2",
    "customize-cra": "^0.8.0",
    "fbemitter": "^2.1.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-app-rewired": "^2.1.4",
    "react-dom": "^16.11.0",
    "react-scripts": "5.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "webrtc-adapter": "^7.3.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
